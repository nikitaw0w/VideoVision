<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bazhenov.nikita.petrsu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="bazhenov.nikita.petrsu.EditCameraActivity"
            android:parentActivityName=".CamersActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CamersActivity" />
        </activity>

        <activity
            android:name="bazhenov.nikita.petrsu.AddCameraActivity"
            android:parentActivityName=".CamersActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CamersActivity" />
        </activity>

        <activity android:name="bazhenov.nikita.petrsu.CamersActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>